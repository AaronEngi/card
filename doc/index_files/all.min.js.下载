postcardControllers.controller("DoneController",["$rootScope","$scope","$window","$location","$http","$routeParams","Util",function(e,t,r,o,n,i,c){e.leftButtonText="<取消",e.rightButtonText="",e.onHeaderLeftButtonClick=function(){o.path("/order")},e.onHeaderRightButtonClick=function(){},t.orderDetail=function(){o.url("/ordersManager")},e.order||(e.order={},e.order.id=i.orderId),"123"==e.activityId&&setTimeout(function(){var e="http://m.huizuche.com/promotion/didi2016";location.href=e},2e3),n.get("/postcard/checkorderispay?"+c.getQueryStringFromObject({orderId:e.order.id})).success(function(e){e.msg}).error(function(e){alert(e)}),"124"==e.activityId&&n.get("/postcard/freeCountByCurrentUserAndDay?"+c.getQueryStringFromObject({actId:e.activityId,userName:e.username})).success(function(e){var t=parseInt(e.count),r=parseInt(e.totalCount);3e3>r&&3>t&&3e3-r>=3-t-1&&alert("您今天还能免费制作"+(3-t-1)+"张明信片！")}).error(function(e){alert(e)}),n.get("/postcard/complete/"+e.order.id+"?"+c.getQueryStringFromObject({nonce:c.getNonceStr()})).success(function(e){}).error(function(){}),n.get("/postcard/getWeixinConfig?"+c.getQueryStringFromObject({})).success(function(e){c.configWeixin(e.config)}).error(function(){}),wx.ready(function(){var t="您也想做这样的DIY明信片吗？赶紧关注服务号趣邮吧，寄出有温度的明信片，传递摸得着的祝福。";"124"==e.activityId&&(t="想DIY你的专属明信片吗？赶快关注【上海K11】 “邮”你所爱！");var r={title:"进来看看我新做的明信片吧，喜欢就赞我哦",desc:t,link:"http://"+o.host()+":"+o.port()+"/client/index.html#/share?orderId="+e.order.id,imgUrl:e.picurl,success:function(){},cancel:function(){}};wx.onMenuShareTimeline(r),wx.onMenuShareAppMessage(r)}),c.overlay.init("<img style='width: 100%' src='images/share-to.png?10001'/>"),t.shareToFriend=function(){c.overlay.show()}}]);
postcardControllers.controller("ShowRequestAddressController",["$rootScope","$scope","$window","$location","$http","$routeParams","Util",function(e,t,n,o,r,a,c){e.leftButtonText="<取消",e.rightButtonText="",e.onHeaderLeftButtonClick=function(){WeixinJSBridge&&WeixinJSBridge.call("closeWindow")},e.onHeaderRightButtonClick=function(){},e.username=a.username,r.get("/contact/getContact",{params:{userName:e.username,contactName:a.contactname}}).success(function(e){var n=e.data;t.name=n.contactName;var o=c.getProvinceAndCityFromAddress(n.address);o.province&&(t.selectedProvince=o.province,t.selectedCity=o.city,t.address=n.address,t.address=t.address.replace(new RegExp(t.selectedProvince,"g"),""),t.address=t.address.replace(new RegExp(t.selectedCity,"g"),"")),t.mobile=n.mobile,t.zipcode=n.zipCode}).error(function(e){alert(e)}),t.onOkButtonClick=function(){o.url("/friendsManager?username="+e.username)}}]);
postcardControllers.controller("ScanToPayController",["$rootScope","$scope","$window","$location","$http","$routeParams","Util",function(t,o,e,r,n,a,c){t.leftButtonText="",t.rightButtonText="",t.onHeaderLeftButtonClick=function(){},t.onHeaderRightButtonClick=function(){};var i=new QRCode(document.getElementById("qrcode"),{width:150,height:150});n.get("/wxpay/payQRpara/",{params:{}}).success(function(t){return 0!=t.code?void alert(t.msg):(o.price=(t.data.price/100).toFixed(2),void i.makeCode(t.data.codeURL))}).error(function(t){alert(JSON.stringify(t)),alert("获取支付参数失败")})}]);
var Postcard=angular.module("Postcard",["ngTouch","ngRoute","ngAnimate","PostcardControllers"],["$httpProvider",function(e){e.defaults.headers.post["Content-Type"]="application/x-www-form-urlencoded;charset=utf-8";var t=function(e){var r,l,o,n,s,a,m,d="";for(r in e)if(l=e[r],l instanceof Array)for(m=0;m<l.length;++m)s=l[m],o=r+"["+m+"]",a={},a[o]=s,d+=t(a)+"&";else if(l instanceof Object)for(n in l)s=l[n],o=r+"["+n+"]",a={},a[o]=s,d+=t(a)+"&";else void 0!==l&&null!==l&&(d+=encodeURIComponent(r)+"="+encodeURIComponent(l)+"&");return d.length?d.substr(0,d.length-1):d};e.defaults.transformRequest=[function(e){return angular.isObject(e)&&"[object File]"!==String(e)?t(e):e}]}]);Postcard.config(["$routeProvider",function(e){e.when("/",{templateUrl:"templates/selectTemplate.html",controller:"SelectTemplateController"}).when("/editInfo",{templateUrl:"templates/editInfo.html",controller:"EditInfoController"}).when("/editContact",{templateUrl:"templates/editContact.html",controller:"EditContactController"}).when("/addressBook",{templateUrl:"templates/addressBook.html",controller:"AddressBookController"}).when("/editGreetings",{templateUrl:"templates/editGreetings.html",controller:"EditGreetingsController"}).when("/selectGreetings",{templateUrl:"templates/selectGreetings.html",controller:"SelectGreetingsController"}).when("/selectPostmark",{templateUrl:"templates/selectPostmark.html",controller:"SelectPostmarkController"}).when("/preview",{templateUrl:"templates/preview.html",controller:"PreviewController"}).when("/order",{templateUrl:"templates/order.html",controller:"OrderController"}).when("/friendsManager",{templateUrl:"templates/friendsManager.html",controller:"FriendsManagerController"}).when("/addContact/:index",{templateUrl:"templates/addContact.html",controller:"AddContactController"}).when("/ordersManager",{templateUrl:"templates/ordersManager.html",controller:"OrdersManagerController"}).when("/continueOrder",{templateUrl:"templates/continueOrder.html",controller:"ContinueOrderController"}).when("/like",{templateUrl:"templates/like.html",controller:"LikeController"}).when("/share",{templateUrl:"templates/share.html",controller:"ShareController"}).when("/requestAddress",{templateUrl:"templates/requestAddress.html",controller:"RequestAddressController"}).when("/showRequestAddress",{templateUrl:"templates/showRequestAddress.html",controller:"ShowRequestAddressController"}).when("/done",{templateUrl:"templates/done.html",controller:"DoneController"}).when("/scanToPay",{templateUrl:"templates/scanToPay.html",controller:"ScanToPayController"}).when("/editSender",{templateUrl:"templates/editSender.html",controller:"EditSenderController"}).when("/requestAddressSuccess",{templateUrl:"templates/requestAddressSuccess.html",controller:"requestAddressSuccessController"}).otherwise({templateUrl:"templates/404.html"})}]);